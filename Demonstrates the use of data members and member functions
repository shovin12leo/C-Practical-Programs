#include<iostream>
#include<string.h>
using namespace std;

#define MAX_CHAR 30

class person {
    private:
        char name[MAX_CHAR]; // Fixed: Added name for the char array
        int age;
    public:
        void get(char n[], int a) {
            strcpy(name, n);
            age = a;
        }
        void put() {
            cout << "Name: " << name << endl;
            cout << "Age: " << age << endl;
        }
};

int main() {
    person PER; // Fixed: Created an instance of the person class
    PER.get("Shovin", 18);
    PER.put();
    return 0;
}
